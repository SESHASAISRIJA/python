def char_frequency(s):
    freq = {}
    for char in s:
        if char in freq:
            freq[char] += 1
        else:
            freq[char] = 1
    return freq

s = input()
print(f"Character frequencies: {char_frequency(s)}")

                (or)
s=input()
c=input()
f=0
f=s.count(c)
print(f)
